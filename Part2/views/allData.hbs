<h1>Patient Homepage</h1>
<ul>

  {{#each data}}
  <li><a href="/patient/{{this._id}}">{{this.firstName}} {{this.lastName}}</a></li>

  <button>
    <a href="/patient/entry/{{this._id}}"> Entry </a>
  </button>

  <button>
    <a href="/patient/view/{{this._id}}"> View </a>
  </button>

  {{/each}}

</ul>